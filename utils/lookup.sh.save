#!/usr/bin/bash

function check()
{
  export prot=$(grep -w $1 swath-ms.merge | cut -f5 | sed 's/_invn//g')
  if [ "${prot}" == "" ]; then
    echo Empty
  else
    grep -H -w ${prot} $INF/doc/hgTables.tsv
    grep -H -w INTERVAL-box.tsv
  fi
}

function Sun()
{
  cut -f5,6 swath-ms.merge | sed '1d;s/_invn//g' | \
  parallel -C' ' '
    echo --- ${rsid} ---
    grep -w {1} pQTL.Sun-B_pQTL_EUR_2017 | grep -w {2}
    if [ "${prot}" == "" ]; then
      echo Empty
    else
      grep -H -w {1} $INF/doc/hgTables.tsv
      grep -H -w {1} 









INTERVAL-box.tsv
    fi
  '
}

function Folkersen()
{
  grep $1 -H -w pQTL.Folkersen-L_Proteins_EUR_2017
  check $1
}

function Suhre()
{
  grep $1 -H -w pQTL.Suhre-K_pQTL_EUR_2017
  check $1
}

function pQTL()
{
  grep $1 -H -w pQTL.pQTL_2017
  check $1
}

Sun
